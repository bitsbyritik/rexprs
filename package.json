{
  "name": "rexprs",
  "version": "0.0.3",
  "description": "Fast Express.js-style web framework powered by Rust",
  "private": true,
  "workspaces": [
    "crates/rexprs-js",
    "cli"
  ],
  "scripts": {
    "build": "cargo build --release && pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "cargo test --workspace && pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "clean": "cargo clean && pnpm -r clean"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "keywords": [
    "rust",
    "express",
    "web-framework",
    "napi",
    "typescript",
    "bindings"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bitsbyritik/rexpres"
  },
  "author": "Ritik Singh <https://x.com/bitsbyritik>"
}
